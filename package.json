{
  "name": "robot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "build": "tsc && vite build",
    "build:ghpages": "tsc && vite build --base /robot/",
    "lint": "biome check --write",
    "format": "biome format --write",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "preview": "vite preview",
    "dev": "vite"
  },
  "dependencies": {
    "pixi.js": "^8.8.1",
    "vitest": "^3.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.6",
    "@eslint/js": "^9.21.0",
    "@vitest/ui": "^3.2.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0"
  },
  "packageManager": "pnpm@10.11.0"
}
